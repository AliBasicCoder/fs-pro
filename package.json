    "mocha-parallel-tests": "^2.3.0",
    "rimraf": "^3.0.2",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.7.0",
    "typedoc": "^0.17.1",
    "typescript": "^3.8.3",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11"
