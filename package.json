{
  "name": "fs-pro",
  "version": "2.6.0",
  "description": "working with files easily",
  "main": "dist/fs-pro.min.js",
  "types": "dist/fs-pro.d.ts",
  "repository": "https://github.com/AliBasicCoder/fs-pro.git",
  "author": "ali_ahmed",
  "license": "MIT",
  "homepage": "https://fs-pro-docs.herokuapp.com/redirect?from=nry&to=/",
  "scripts": {
    "docs": "typedoc --out ../fs-pro-docs/website && cd ../fs-pro-docs && csso website/assets/css/main.css --output website/assets/css/main.css && git add . && git commit -a -m \"new version\" && git push origin master",
    "lint": "eslint src/*.ts",
    "build": "webpack && dts-bundle --configJson dts-bundle.json && rimraf types",
    "git": "git add . && git commit -a -m \"new version\" && git push origin master",
    "test": "mocha -r ts-node/register ./test/test.ts",
    "cpublish": "yarn build && yarn test && yarn lint dist/*.d.ts --fix && yarn docs && yarn publish"
  },
  "devDependencies": {
    "@types/mocha": "^7.0.2",
    "@typescript-eslint/eslint-plugin": "^2.24.0",
    "@typescript-eslint/parser": "^2.24.0",
    "dts-bundle": "^0.7.3",
    "eslint": "^6.8.0",
    "mocha": "^8.0.1",
    "rimraf": "^3.0.2",
    "ts-loader": "^7.0.0",
    "ts-node": "^9.0.0",
    "typedoc": "^0.17.1",
    "typescript": "^4.0.2",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11"
  },
  "dependencies": {
    "@types/node": "^14.11.1",
    "node-watch": "^0.6.3"
  },
  "keywords": [
    "fs",
    "file-system",
    "watch",
    "object",
    "read",
    "write",
    "json"
  ]
}
